let
    RW ← (
        ( fix f . λ x . in ( [ x , f ⊙ next ( normal [x , s] ) ] ) )
    );
    head ← (
        λ s . fst (out (unbox s))
    );
    tail ← (
        λ s . boxI ( prevI ( snd ( out ( ( unbox s ) ) ) ) )
    );
    getIdx ← (
        fix f . λ idx . λ rw . if ( idx <= 0.0 )
            then head rw
            else prev (f ⊙ next (idx - 1.0) ⊙ next (tail rw))
    )
in: ( getIdx idx ) ( boxI ( RW x0 ))
